{
  "name": "i18n-pro",
  "version": "1.0.0",
  "description": "International tool",
  "files": [
    "dist",
    "template",
    "locale/.log/langs.json"
  ],
  "main": "dist/lib/index.js",
  "bin": {
    "i18n": "dist/bin/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "npx tsc --watch",
    "test-init": "node ./dist/bin/index.js init",
    "test-t": "node ./dist/bin/index.js -T",
    "test-translate": "node ./dist/bin/index.js translate",
    "publish": "npm publish --access=public --registry https://registry.npmjs.org",
    "lint-code": "npx lint-staged",
    "lint-msg": "npx commitlint --edit"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "国际化",
    "多语言"
  ],
  "author": "Eyelly Wu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@eyelly/prettier-config": "^1.0.0",
    "@types/got": "^9.6.12",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.41",
    "@types/walk": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8",
    "lint-staged": "^13.0.1",
    "md5-node": "^1.0.1",
    "prettier": "^2.6.2",
    "typescript": "4",
    "walk": "^2.3.15"
  },
  "dependencies": {
    "chalk": "4",
    "g": "^2.0.1",
    "got": "11",
    "lodash": "^4.17.21"
  },
  "lint-staged": {
    "*.{json,ts}": "eslint --fix -f table"
  }
}
